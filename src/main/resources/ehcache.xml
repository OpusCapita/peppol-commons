<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ehcache>
<ehcache>
    <cache name="auth.token"
           eternal="false"
           overflowToDisk="false"
           timeToIdleSeconds="3600"
           timeToLiveSeconds="3600"
           maxElementsInMemory="10"
    />
    <cache name="validation-xsd"
           eternal="true"
           overflowToDisk="true"
           maxElementsInMemory="100"
    />
    <cache name="validation-xsl"
           eternal="true"
           overflowToDisk="true"
           maxElementsInMemory="100"
    />
</ehcache>